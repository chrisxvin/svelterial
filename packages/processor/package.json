{
  "name": "@svelterial/processor",
  "version": "0.0.0",
  "description": "A svelte preprocessor for svelterial plugins.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheComputerM/svelterial.git",
    "directory": "packages/processor"
  },
  "author": "TheComputerM",
  "homepage": "https://github.com/TheComputerM/svelterial#readme",
  "bugs": {
    "url": "https://github.com/TheComputerM/svelterial/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "node": "./dist/index.cjs"
  },
  "scripts": {
    "build": "pnpm types && rollup -c",
    "test": "jest",
    "types": "dts-bundle-generator -o dist/index.d.ts src/index.ts"
