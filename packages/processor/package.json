  "files": [
    "dist/**"
  ],
  "keywords": [
    "svelte",
    "sveltejs",
    "preprocessor",
    "svelterial"
  ],
  "dependencies": {
    "deepmerge": "^4.2.2",
    "postcss": "^8.2.8",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "dts-bundle-generator": "^5.8.0",
    "is-plain-object": "^5.0.0",
    "jest": "^26.6.3",
    "rollup": "^2.42.3",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "types": "dist/index.d.ts"
}
